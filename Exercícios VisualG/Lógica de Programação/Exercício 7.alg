algoritmo "semnome"
// Função :
// Autor :
// Data : 30/12/2019
// Seção de Declarações 
var

numero, n: inteiro

inicio
// Seção de Comandos

numero <- randi (5)+1
escreva("Digite um numero: ")
leia(n)

  se(n = numero)entao
    escreval("Parabens, vc acertou!")
  senao
    escreval("Vc errou!")
  Fimse

fimalgoritmo
